set title "Elsasser number" font ",20"
set key left
plot [-0.05:1.05] "data_q0" u 1:4 w lp lc 1, "data_q0" u 1:4:6 w yerrorbars lc 1 notitle
replot "data_q1" u 1:4 w lp lc 3, "data_q1" u 1:4:6 w yerrorbars lc 3 notitle
replot "data_q2" u 1:4 w lp lc 0, "data_q2" u 1:4:6 w yerrorbars lc 0 notitle
pause -1